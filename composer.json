{
    "name": "loldev/zend-queue",
    "description": "Provides a factory function to create specific queue client objects",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf2",
        "queue"
    ],
    "homepage": "https://github.com/loldev/ZendQueue",
    "autoload": {
        "psr-0": {
            "ZendQueue": "library/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.zendframework.com/"
        },
        {
            "type": "vcs",
            "url": "https://github.com/pruno/phpunit"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "3.7.99"
    },
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "2.2.*"
    },
    "suggest": {
        "zendframework/zend-db": "Zend\\Db required if you are using the DB queue adapter"
    }
}
