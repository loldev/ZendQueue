{
    "name": "loldev/zend-queue",
    "description": "Provides a factory function to create specific queue client objects",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf2",
        "queue"
    ],
    "homepage": "https://github.com/ripaclub/ZendQueue",
    "autoload": {
        "psr-0": {
            "ZendQueue": "library/"
        }
    },
	"require-dev": {
        "phpunit/phpunit": "3.7.*",
        "zendframework/zend-config": "2.2.*"
    },
    "require": {
        "php": ">=5.3.3",
        "zendframework/zend-stdlib": "2.2.*",
        "zendframework/zend-servicemanager": "2.2.*",
        "zendframework/zend-math": "2.2.*",
        "zendframework/zend-db": "2.2.*",
        "zendframework/zend-eventmanager": "2.2.*"
    },
    "suggest": {
        "zendframework/zend-db": "Zend\\Db required if you are using the DB queue adapter"
    }
}
